{
  "name": "@valtioinc/valtio-react-sdk",
  "description": "Valtio React SDK",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "author": "Valtio Inc",
  "homepage": "https://valtio.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valtioinc/valtio-react-sdk.git"
  },
  "keywords": [
    "valtio",
    "sdk",
    "react"
  ],
  "source": "./src/index.tsx",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!lib/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./css": {
      "import": "./lib/index.css",
      "require": "./lib/index.css"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./lib ./coverage",
    "prepack": "yarn clean && node esbuild.config.js && tsc --emitDeclarationOnly",
    "typecheck": "tsc",
    "lint": "eslint \"{src,tests}/**/*.{js,ts,tsx}\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "sca": "audit-ci --config .audit-ci.jsonc --report-type summary",
    "sca:fix": "yarn-audit-fix",
    "release": "semantic-release"
  },
  "packageManager": "yarn@4.3.0",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "audit-ci": "^7.1.0",
    "c8": "^10.1.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^14.0.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.2.0",
    "typescript": "^5.2.2",
    "vitest": "^2.1.8",
    "yarn-audit-fix": "^10.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
